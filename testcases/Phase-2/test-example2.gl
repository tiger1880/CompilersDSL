point p := (2,3), q := (6,8)
repeat(int i:= ||p||+||q|| | i < 20  | i++)
{
    if(i%2==0 and (i > 5 and i<15))
    {
        print(||p-q||)
    }
    else
    {
        int dist := SHORTEST_DISTANCE(p,q)
        dist++
        stop
    }
}

